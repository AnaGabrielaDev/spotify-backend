// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String

  @@map("users")
}

model Music {
  id Int @id @default(autoincrement())
  name String
  url String
  thumbnail String

  @@map("musics")
}
